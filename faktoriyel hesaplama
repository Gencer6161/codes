#include <iostream>
#include <stdio.h>
#include <ctime>
#pragma warning (disable : 4996)
using namespace std;

int main() {
	int sayı;
	int faktoriyel = 1;  

	printf("Faktoriyali hesaplanacak sayıyı giriniz: ");
	cin >> sayı;

	for (int i = 1; i <= sayı; i++) {
		faktoriyel *= i;
	}

	cout << faktoriyel << endl;

	return 0;
}
