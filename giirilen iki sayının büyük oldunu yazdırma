#include  <iostream>
#include  <stdio.h>
#include  <ctime>
#include  <locale.h>
#pragma warning (disable : 4996)
using namespace std;

void fonk() {
	int a;
	int b;

	cout << "değer giriniz:";
	cin >> a;
	cout << "değer giriniz:";
	cin >> b;
	if (a < b) {
		cout << b;
		
	}
	if (b < a) {
		cout << a;
	}

}



int main() {
	setlocale(LC_ALL, "Turkish");
	fonk();
	return 0;
}
