#include  <iostream>
#include  <stdio.h>
#include  <ctime>
#include  <locale.h>
#pragma warning (disable : 4996)
using namespace std;

int fonk() {
	int x, y;
	cout << "değer giriniz";
	cin >> x;
	cout << "değer giriniz";
	cin >> y;
	for (x+1; x > y+1; x--) {
		int c;
		c = x - 1;
		cout << c;
			cout << "\n";
	}for (y+1; y >x ; y--) {
		int b;
		b = y - 1;
		cout << b;
		cout << "\n";
	}
	
	return 0;
}


int main() {
	setlocale(LC_ALL, "Turkish");
	fonk();
	return 0;
}
