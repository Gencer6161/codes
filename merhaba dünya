#include  <iostream>
#include  <stdio.h>
#include  <ctime>
#include  <locale.h>
#pragma warning (disable : 4996)
using namespace std;

int yazdir() {

	int sayaç = 0;
	while (sayaç < 10) {

		cout << "Merhaba dünya";
		printf("\n");
		sayaç++;
	}



	return 0;
}


int main() {
	setlocale(LC_ALL, "Turkish");
	int a = yazdir();
	cout << a;
	return 0;
}
