#include  <iostream>
#include  <stdio.h>
#include  <ctime>
#include  <locale.h>
#pragma warning (disable : 4996)
using namespace std;

int fonk(int a,int b); 
if (a < b) {
	cout << b;

}
if (b < a) {
	cout << a;

	return 0;
}



int main() {
	setlocale(LC_ALL, "Turkish");
	int a;
	int b;

	cout << "değer giirniz";
	cin >> a;
	cout << "değer giirniz";
	cin >> b;
	fonk(a, b);

	return 0;
}
