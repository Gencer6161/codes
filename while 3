#include <iostream>
#include <stdio.h>
#include <ctime>
#pragma warning (disable : 4996)
using namespace std;
int main() {
	int sayaç = 500;
	while (sayaç <= 1000000000000) {
		cout << sayaç;
		sayaç = sayaç +1;
		printf("\n");
	}
	while (sayaç >= 200) {
		cout << sayaç;
		sayaç = sayaç - 1;
		printf("\n");
	}
	return 0;
}
